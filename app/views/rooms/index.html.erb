<div class="container">
  <h1>All rooms</h1>
  <div class="table">
    <table>
      <thead>
        <tr>
          <th>image</th>
          <th>name</th>
          <th>introduction</th>
          <th>fee</th>
          <th>address</th>
          <th>created date</th>
          <th>edit</th>
        </tr>
      </thead>

      <tbody>
        <% @room.each do |room| %>
          <tr>
            <td><%= room.image %></td>
            <td><%= link_to room.name, room_path(room.id) %></td>
            <td><%= room.room_introduction %></td>
            <td><%= room.fee %></td>
            <td><%= room.address %></td>
            <td><%= room.created_at.strftime('%Y/%m/%d') %></td>
            <td><%= link_to "Edit", [:edit, room] %></td>
          </tr>
        <% end %>
      </tbody>
    </table>
  </div>
</div>